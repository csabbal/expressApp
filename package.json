  "description": "very basic express ts app",
  "main": "index.ts",
  "scripts": {
    "build": "webpack",
    "start:dev": "NODE_ENV=development  webpack",
    "run:dev": "NODE_ENV=development nodemon build/index.js",
    "debug:dev": "NODE_ENV=development nodemon --inspect build/index.js",
    "test:coverage": "nyc npm test",
    "test": "TS_NODE_PROJECT=tsconfig.test.json node --loader ts-node/register ./node_modules/.bin/mocha src/**/*.spec.ts",
    "test:watch": "TS_NODE_PROJECT=tsconfig.test.json node --loader ts-node/register ./node_modules/.bin/mocha  --watch --watch-files  src/**/*.spec.ts --watch-ignore node_modules ",
